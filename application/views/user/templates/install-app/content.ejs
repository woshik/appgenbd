<div class="outer-w3-agile col-xl mt-3">
	<h4 class="tittle-w3-agileits mb-4"><%=title%></h4>
	<form action="<%=installAppFormURL%>" method="post" id="installAppForm">
		<div id="message"></div>
		<div class="form-group row">
			<label class="col-sm-2 col-form-label">Host Address</label>
			<div class="col-sm-10">
				<input type="text" class="form-control" value="<%=hostAddress%>" readonly />
			</div>
		</div>
		<div class="form-group row">
			<label for="appName" class="col-sm-2 col-form-label">App Name</label>
			<div class="col-sm-10">
				<input type="text" name="appName" class="form-control" id="appName" placeholder="App Name" autocomplete="off" />
			</div>
		</div>
		<div id="sectionHidden">
			<div class="form-group row">
				<label for="smsUrl" class="col-sm-2 col-form-label">Message Receiving URL</label>
				<div class="col-sm-10">
					<input type="text" class="form-control" id="smsUrl" readonly />
				</div>
			</div>
			<div class="form-group row">
				<label for="ussdUrl" class="col-sm-2 col-form-label">Ussd Connection URL</label>
				<div class="col-sm-10">
					<input type="text" class="form-control" id="ussdUrl" readonly />
				</div>
			</div>
			<div class="form-group row">
				<label for="providerId" class="col-sm-2 col-form-label">App Id</label>
				<div class="col-sm-10">
					<input type="text" name="providerId" class="form-control" id="providerId" placeholder="App Id" autocomplete="off" />
				</div>
			</div>
			<div class="form-group row">
				<label for="providerPassword" class="col-sm-2 col-form-label">App Password</label>
				<div class="col-sm-10">
					<input type="text" name="providerPassword" class="form-control" id="providerPassword" placeholder="App Password" autocomplete="off" />
				</div>
			</div>
		</div>
		<input type="hidden" name="appId" id="appId" />
		<div style="text-align: center;">
			<button type="submit" class="btn btn-primary" id="buttonload">Submit</button>
		</div>
	</form>
</div>
