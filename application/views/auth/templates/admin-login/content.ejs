<form action="<%=loginForm%>" method="POST" class="login100-form validate-form" id="loginForm">
	<span class="login100-form-title p-b-14">
		Welcome
	</span>
	<span class="login100-form-title p-b-34">
		<%=title%>
	</span>

	<% if (successMessage) { %>
	<div class="alert alert-success alert-dismissible" role="alert" style="width: 100%;">
		<%=successMessage%>
	</div>
	<% } %>
	<div id="message" style="width: 100%"></div>
	<div class="wrap-input100 rs1-wrap-input100 validate-input m-b-20" data-validate="Type email">
		<input id="first-name" class="input100" type="text" name="email" placeholder="Email" autocomplete="off">
		<span class="focus-input100"></span>
	</div>
	<div class="wrap-input100 rs2-wrap-input100 validate-input m-b-20" data-validate="Type password" autocomplete="off">
		<input class="input100" type="password" name="password" placeholder="Password">
		<span class="focus-input100"></span>
	</div>
	<div class="container-login100-form-btn">
		<button class="login100-form-btn" id="buttonload">
			Sign in
		</button>
	</div>
</form>
