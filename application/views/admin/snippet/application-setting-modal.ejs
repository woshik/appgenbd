<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="appSettingModalLabel" aria-hidden="true" id="appSetting">
	<div class="modal-dialog modal-lg">
		<div class="modal-content">
			<form action="/application-setting" method="post" id="applicationSettingForm">
				<div class="modal-header">
					<h4 class="modal-title" id="appSettingModalLabel">App Setting</h4>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">Ã—</span>
					</button>
				</div>
				<div class="modal-body">
					<div id="application-setting-message"></div>
					<div class="form-group row">
						<label for="maxAppInstall" class="col-sm-4 col-form-label">Default Max App Can Install</label>
						<div class="col-sm-8">
							<input type="number" class="form-control" name="maxAppCanInstall" id="maxAppCanInstall" placeholder="Default Max App" autocomplete="off">
						</div>
					</div>
					<div class="form-group row">
						<label for="costPerMonth" class="col-sm-4 col-form-label">Cost Per Month</label>
						<div class="col-sm-8">
							<input type="number" class="form-control" name="costPerMonth" id="costPerMonth" placeholder="Cost Per Month" autocomplete="off">
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button type="submit" class="btn btn-success" id="applicationSettingBtn">Change</button>
				</div>
			</form>
		</div>
	</div>
</div>