<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="appSettingModalLabel" aria-hidden="true" id="appSetting">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <form class="<%=applicationSetting=%>" action="index.html" method="post">
                <div class="modal-header">
                    <h4 class="modal-title" id="appSettingModalLabel">App Setting</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">Ã—</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div id="application-setting-message"></div>
                    <div class="form-group row">
                        <label for="maxAppInstall" class="col-sm-3 col-form-label">Default Max App</label>
                        <div class="col-sm-9">
                            <input type="number" class="form-control" id="maxAppInstall" placeholder="Default Max App">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="costPerMonth" class="col-sm-3 col-form-label">Cost Per Month</label>
                        <div class="col-sm-9">
                            <input type="number" class="form-control" id="costPerMonth" placeholder="Cost Per Month">
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-success" id="appSettingButton">Change</button>
                </div>
            </form>
        </div>
    </div>
</div>